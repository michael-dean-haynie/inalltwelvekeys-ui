{
  "name": "inalltwelvekeys-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:dev": "ng build --configuration=development",
    "watch": "ng build --watch --configuration development --output-path ../inalltwelvekeys/inalltwelvekeys-ui",
    "test": "ng test",
    "deploy": "sudo rm -rf ./dist && sudo npm run build && sudo rm -rf ../inalltwelvekeys/inalltwelvekeys-ui && sudo cp -r ./dist/inalltwelvekeys-ui ../inalltwelvekeys/inalltwelvekeys-ui",
    "deploy:dev": "sudo rm -rf ./dist && sudo npm run build:dev && sudo rm -rf ../inalltwelvekeys/inalltwelvekeys-ui && sudo cp -r ./dist/inalltwelvekeys-ui ../inalltwelvekeys/inalltwelvekeys-ui"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.8",
    "@angular/common": "^17.0.8",
    "@angular/compiler": "^17.0.8",
    "@angular/core": "^17.0.8",
    "@angular/forms": "^17.0.8",
    "@angular/platform-browser": "^17.0.8",
    "@angular/platform-browser-dynamic": "^17.0.8",
    "@angular/router": "^17.0.8",
    "@tonejs/piano": "^0.2.1",
    "midi-writer-js": "^3.1.1",
    "piano-chart": "^1.5.0",
    "rxjs": "~7.8.0",
    "tonal": "^5.0.0",
    "tone": "^14.7.77",
    "tslib": "^2.3.0",
    "uuid": "^9.0.1",
    "vexflow": "^4.2.3",
    "webmidi": "^2.5.1",
    "webmidi3": "npm:webmidi@^3.1.6",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.9",
    "@angular/cli": "~17.0.9",
    "@angular/compiler-cli": "^17.0.8",
    "@types/jasmine": "~4.3.0",
    "@types/uuid": "^9.0.4",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.2.2"
  }
